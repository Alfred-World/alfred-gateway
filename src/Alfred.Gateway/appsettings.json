{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "AllowedHosts": "*",
  "Auth": {
    "Authority": "http://localhost:5001",
    "ValidIssuer": "Alfred.Identity",
    "RequireHttpsMetadata": false
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:5173"
    ]
  },
  "RateLimit": {
    "Window": "00:01:00",
    "PermitLimit": 100,
    "QueueLimit": 2
  }
}
